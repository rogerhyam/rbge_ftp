<?xml version="1.0" standalone="yes"?>
<archive xmlns="http://rs.tdwg.org/dwc/text/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:schemaLocation="http://rs.tdwg.org/dwc/text/ http://rs.tdwg.org/dwc/text/tdwg_dwc_text.xsd" metadata="prov.xml">
    <core encoding="UTF-8" fieldsTerminatedBy="," linesTerminatedBy="\n" fieldsEnclosedBy='"' ignoreHeaderLines="1" rowType="http://rs.tdwg.org/dwc/terms/Occurrence">
        <files>
            <location>herbarium_specimens.csv</location>
        </files>

        <!-- record id is guid -->
        <id index="0" />

        <!-- dynamic fields -->
        {{DYNAMIC_FIELDS}}
       
        <!-- static values -->
        <field term="http://rs.tdwg.org/dwc/terms/nomenclaturalCode" default="ICBN" />
        <field term="http://purl.org/dc/terms/type" default="PhysicalObject" />
        <field term="http://rs.tdwg.org/dwc/terms/ownerInstitutionCode" default="E" />
        <field term="http://rs.tdwg.org/dwc/terms/geodeticDatum" default="wgs84" />
        <field term="http://rs.tdwg.org/dwc/terms/institutionCode" default="RBGE" />
        <field term="http://rs.tdwg.org/dwc/terms/institutionID" default="https://scientific-collections.gbif.org/institution/0237598a-853a-492c-af74-a723fe251799" />
        <field term="http://rs.tdwg.org/dwc/terms/collectionID" default="https://scientific-collections.gbif.org/collection/427c8cd7-4358-4a00-9ef3-2b2676d28d1e" />
        <field term="http://rs.tdwg.org/dwc/terms/collectionCode" default="E" />
        <field term="http://rs.tdwg.org/dwc/terms/datasetName" default="Edinburgh (E) Herbarium Specimens (selected by filtering by barcode starts with E)" />
        <field term="http://rs.tdwg.org/dwc/terms/basisOfRecord" default="Preserved specimen" />
        <field term="http://purl.org/dc/elements/1.1/type" default="PhysicalObject" />
        <field term="http://rs.tdwg.org/dwc/terms/license" default="CC0" />
        <field term="http://rs.tdwg.org/dwc/terms/rightsHolder" default="Royal Botanic Garden Edinburgh" />   
        
    </core>


    <extension encoding="UTF-8" fieldsTerminatedBy="," linesTerminatedBy="\n" fieldsEnclosedBy='"' ignoreHeaderLines="0" rowType="http://rs.tdwg.org/ac/terms/Multimedia">
        <files>
            <location>herbarium_specimen_images.csv</location>
        </files>
        <coreid index="0"/>
        <field index="1" term="http://purl.org/dc/elements/1.1/type"/>
        <field index="2" term="http://purl.org/dc/elements/1.1/format"/>
        <field index="3" term="http://rs.tdwg.org/ac/terms/accessURI"/>
        <field index="4" term="http://rs.tdwg.org/ac/terms/associatedSpecimenReference"/>
        <field index="5" term="http://purl.org/dc/terms/identifier"/>
        <field index="6" term="http://purl.org/dc/terms/description"/>
        <field index="7" term="http://rs.tdwg.org/ac/terms/serviceExpectation"/>

        <!-- static values -->
        <field term="http://ns.adobe.com/xap/1.0/rights/Owner" default="Royal Botanic Garden Edinburgh"/>
        <field term="http://purl.org/dc/terms/rights" default="https://creativecommons.org/publicdomain/zero/1.0/"/>
        <field term="http://purl.org/dc/elements/1.1/rights" default="Copyright Royal Botanic Garden Edinburgh."/>
        <field term="http://rs.tdwg.org/dwc/terms/rightsHolder" default="Royal Botanic Garden Edinburgh" />   
        <field term="http://purl.org/dc/elements/1.1/creator" default="Royal Botanic Garden Edinburgh"/>
        <field term="http://rs.tdwg.org/dwc/terms/license" default="CC BY" />
       
    </extension>

</archive>
